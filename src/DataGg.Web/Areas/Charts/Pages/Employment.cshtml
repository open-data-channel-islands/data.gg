@page "/Charts/Employment"

@{
    ViewData["Title"] = "Employment";
}


@model DataGg.Web.Areas.Charts.Pages.EmploymentModel


<!-- Nav tabs -->
<ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="totals-tab" data-toggle="tab" href="#totals" role="tab" aria-controls="totals" aria-selected="true">Totals</a>
    </li>
</ul>

<!-- Tab panes -->
<div class="tab-content">
    <div class="tab-pane active" id="totals" role="tabpanel" aria-labelledby="totals-tab">
        @await Component.InvokeAsync("EmploymentTotalsChart", new { })
    </div>
</div>